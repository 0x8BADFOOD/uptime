<tr class="<%= check.isUp ? 'green' : 'red' %>">
  <td><%= link_to(check.name, route + '/check/' + check._id) %> <%= link_to(img_tag(route + '/images/external-link-ltr-icon.png', check.url), check.url) %></td>
  <td><span class="label <%= (check.isUp ? 'success' : 'important') %>"><%= (check.isUp ? 'yes' : 'no') %></span></td>
  <td><%= (check.isUp ? check.uptime : check.downtime) %></td>
</tr>